<h1><%= @team.name %></h1>
<br>

<h3>Members</h3>
<br>
<% @team.members.each do |member| %>
  <%= link_to member.name, user_url(member) %>
  <br>
<% end %>


<h3>Projects</h3>
<br>
<% @team.projects.each do |project| %>
  <%= link_to project.title, project_url(project) %>
  <br>
<% end %>
<br>

<%= link_to "Create new project for this team", new_team_project_url(@team) %>